<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>Sales Invoice</title>
        <link rel="stylesheet" type="text/css" href="style.css"/>
    </h:head>
    <h:body>
        <!-- INVOICE STARTS HERE -->
        <p:commandButton id="cmdbPrint" styleClass="clsButtonBigLonger" value="Print Sales Invoice Below (ALT+P)" icon="ui-icon-print" type="button" accesskey="p">
            <p:printer target="IpgrdOuterSalesInvoice" rendered="false"/>
        </p:commandButton>
        <h:panelGrid id="IpgrdOuterSalesInvoice">
            <h:panelGrid id="IpgrdSalesInvoice" columns="1">

                <h:panelGrid id="IpgrdSalesInvoiceHeader1" style="width: 350px;text-align: center;border-bottom-style: dotted;" columns="1">
                    <h:outputLabel id="IolblSalesInvoiceCompany" style="font-size: 20px;font-weight: bold;" value="#{companySetting.getLICENSE_CLIENT_NAME()}"/>
                    <p:panel style="width: 290px;text-align: center; border-style: none !important;">
                        <h:outputLabel id="IolblSalesInvoiceBranch" value="#{companySetting.getBranchCode()} " rendered="#{generalSetting.isTwoStringsEqual(companySetting.getShowBranchInvoice(),'Yes')}"/>
                        <h:outputLabel id="IolblSalesInvoiceStore" value="(#{storeBean.getStore(salesInvoiceBean.getCurrentTransaction().storeId).storeName})" rendered="#{generalSetting.isTwoStringsEqual(companySetting.getShowStoreInvoice(),'Yes')}"/>
                    </p:panel>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceHeader2" style="width: 350px; margin: 0px;" columns="2">
                    <p:graphicImage id="IgimgSalesInvoiceLogo" value="#{companySetting.getLogoUrl()}" style="width:150px;float: left;" rendered="#{generalSetting.isTwoStringsEqual(companySetting.getShowLogoInvoice(),'Yes')}" />
                    <h:outputLabel id="IolblSalesInvoiceAddress" style="" value="#{companySetting.getPhysicalAddress()}"/>
                </h:panelGrid>
                <h:panelGrid id="IpgrdSalesInvoiceHeader3" style="width: 350px; margin: 0px;" columns="2">
                    <h:outputLabel id="IolblSalesInvoiceTel" value="Tel:#{companySetting.getPhone()}"/>
                    <h:outputLabel id="IolblSalesInvoiceTIN" value="TIN:#{companySetting.getTaxIdentity()}"/>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceHeader34" style="width: 350px; margin: 0px;border-bottom-style: dotted;" columns="1">
                    <h:outputLabel id="IolblSalesInvoiceWebsite" value="Website:#{companySetting.getWebsite()}" rendered="#{companySetting.getWebsite().length()>0}"/>
                    <h:outputLabel id="IolblSalesInvoiceEmail" value="Email:#{companySetting.getEmail()}" rendered="#{companySetting.getEmail().length()>0}"/>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceHeader4" style="width: 350px; margin: 0px;" columns="4">
                    <h:outputLabel style="font-size: 12px;" value="#{companySetting.getSalesReceiptName()}:"/>
                    <h:outputLabel id="IolblSalesInvoiceNo" style="font-size: 20px;font-weight: bold;" value="#{salesInvoiceBean.getCurrentTransaction().transactionId}"/>
                    <h:outputLabel style="font-size: 12px;" value="Date:"/>
                    <h:outputLabel id="IolblSalesInvoiceDate" style="font-size: 12px;" value="#{salesInvoiceBean.getCurrentTransaction().addDate}">
                        <f:convertDateTime pattern="#{companySetting.getDateFormat()} H:mm" timeZone="#{companySetting.getTimeZone()}"/>
                    </h:outputLabel>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceHeader5" style="width: 350px; margin: 0px;" columns="2">
                    <h:outputLabel style="font-size: 12px;" value="Customer:"/>
                    <h:outputLabel id="IolblSalesInvoiceCustomer" style="font-size: 12px;" value="#{salesInvoiceBean.getCurrentTransactor().transactorNames}"/>
                    <h:outputLabel style="font-size: 12px;" value="Bill for:"/>
                    <h:outputLabel id="IolblSalesInvoiceBillCustomer" style="font-size: 12px;" value="#{salesInvoiceBean.getCurrentBillTransactor().transactorNames}"/>
                </h:panelGrid>
                <h:panelGrid id="IpgrdSalesInvoiceItems">
                    <h:dataTable id="IdtblSalesInvoiceItems" value="#{salesInvoiceBean.currentTransactionItems}" var="ti" 
                                 styleClass="cls-table-SalesInvoice" 
                                 headerClass="cls-table-header-SalesInvoice" 
                                 rowClasses="cls-table-odd-row-SalesInvoice,cls-table-even-row-SalesInvoice" 
                                 columnClasses="cls-table-col1-SalesInvoice,cls-table-col2-SalesInvoice,cls-table-col3-SalesInvoice,cls-table-col4-SalesInvoice,cls-table-col5-SalesInvoice"
                                 > 
                        <h:column>
                            <!-- column header -->
                            <f:facet name="header">Item</f:facet>
                            <!-- row record -->
                            #{itemBean.getItem(ti.itemId).description} [#{salesInvoiceBean.getVatRatedCode(ti.vatRated)}]
                        </h:column>
                        <h:column>
                            <!-- column header -->
                            <f:facet name="header">Qty</f:facet>
                            <!-- row record -->
                            <h:outputLabel value="#{ti.itemQty}" style="font-size: 14px;">
                                <f:convertNumber pattern="###,###.##"/>
                            </h:outputLabel> #{unitBean.getUnit(itemBean.getItem(ti.itemId).unitId).unitSymbol}
                        </h:column>
                        <h:column>
                            <!-- column header -->
                            <f:facet name="header">Price</f:facet>
                            <!-- row record -->
                            <h:outputLabel value="#{ti.unitPriceIncVat}" style="font-size: 14px;">
                                <f:convertNumber pattern="###,###.##"/>
                            </h:outputLabel>
                        </h:column>
                        <h:column>
                            <!-- column header -->
                            <f:facet name="header">Disc</f:facet>
                            <!-- row record -->
                            <h:outputLabel value="#{ti.unitTradeDiscount}" style="font-size: 14px;">
                                <f:convertNumber pattern="###,###.##"/>
                            </h:outputLabel>
                        </h:column>
                        <h:column>
                            <!-- column header -->
                            <f:facet name="header">Amount</f:facet>
                            <!-- row record -->
                            <h:outputLabel value="#{ti.amountIncVat}" style="font-size: 14px;">
                                <f:convertNumber pattern="###,###.##"/>
                            </h:outputLabel>
                        </h:column>
                    </h:dataTable>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceTotals" style="width: 350px; margin: 0px;border-bottom-style: dotted" columns="2">
                    <h:outputLabel style="font-size: 16px;" value="SubTotal :"/>
                    <h:outputLabel style="font-size: 16px;" id="IolblSalesInvoiceSubTotal" value="#{salesInvoiceBean.getCurrentTransaction().subTotal}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px;" value="Total Trade Discount :"/>
                    <h:outputLabel style="font-size: 16px;" id="IolblSalesInvoiceTotalTradeDiscount" value="#{salesInvoiceBean.getCurrentTransaction().totalTradeDiscount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px;" value="Total VAT :"/>
                    <h:outputLabel style="font-size: 16px;" id="IolblSalesInvoiceTotalVat" value="#{salesInvoiceBean.getCurrentTransaction().totalVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px;" value="Cash Discount :"/>
                    <h:outputLabel style="font-size: 16px;" id="IolblSalesInvoiceCashDiscount" value="#{salesInvoiceBean.getCurrentTransaction().cashDiscount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" value="GRAND TOTAL :"/>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" id="IolblSalesInvoiceGrandTotal" value="#{salesInvoiceBean.getCurrentTransaction().grandTotal}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px;" value="POINTS AMOUNT SPENT :"/>
                    <h:outputLabel id="IolblSalesInvoicePointsAmountSpent" value="#{salesInvoiceBean.getCurrentPay().pointsSpentAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" value="AMOUNT TENDERED :"/>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" id="IolblSalesInvoiceAmountTendered" value="#{salesInvoiceBean.getCurrentTransaction().amountTendered}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" value="CHANGE AMOUNT :"/>
                    <h:outputLabel style="font-size: 16px; font-weight: bold;" id="IolblSalesInvoiceChangeAmount" value="#{salesInvoiceBean.getCurrentTransaction().changeAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceVAT" style="width: 350px; margin: 0px;border-bottom-style: dotted" columns="4">
                    <h:outputLabel style="font-size: 14px;" value="VAT Rated"/>
                    <h:outputLabel style="font-size: 14px;" value="(%)"/>
                    <h:outputLabel style="font-size: 14px;" value="VatableAmount"/>
                    <h:outputLabel style="font-size: 14px;" value="VAT"/>

                    <h:outputLabel style="font-size: 14px;" value="Standard[S]"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().vatPerc}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalStdVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>

                    <h:outputLabel style="font-size: 14px;" value="Zero[Z]"/>
                    <h:outputLabel style="font-size: 14px;" value="0"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalZeroVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="0"/>

                    <h:outputLabel style="font-size: 14px;" value="Exempt[E]"/>
                    <h:outputLabel style="font-size: 14px;" value="n/a"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalExemptVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="0"/>

                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoicePointsCard" style="width: 350px; margin: 0px;border-bottom-style: dotted" columns="2">
                    <h:outputLabel style="font-size: 14px;" value="Card No :"/>
                    <h:outputLabel style="font-size: 14px;" id="IolblSalesInvoiceCardNo" value="#{salesInvoiceBean.getCurrentPointscard().cardNumber}"/>
                    <h:outputLabel style="font-size: 14px;" value="Card Holder :"/>
                    <h:outputLabel style="font-size: 14px;" id="IolblSalesInvoiceCardHolder" value="#{salesInvoiceBean.getCurrentPointscard().cardHolder}"/>
                    <h:outputLabel style="font-size: 14px;" value="Points Spent :"/>
                    <h:outputLabel style="font-size: 14px;" id="IolblSalesInvoicePointsSpent" value="#{salesInvoiceBean.getCurrentPay().pointsSpent}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="Points Awarded :"/>
                    <h:outputLabel style="font-size: 14px;" id="IolblSalesInvoicePointsAwarded" value="#{salesInvoiceBean.getCurrentTransaction().pointsAwarded}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="Points Balance :"/>
                    <h:outputLabel style="font-size: 14px;" id="IolblSalesInvoicePointsBalance" value="#{salesInvoiceBean.getCurrentPointscard().pointsBalance}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceFooter1" style="width: 350px; margin: 0px;border-bottom-style: dotted" columns="2">
                    <h:outputLabel value="Service offered by : "/>
                    <h:outputLabel value="#{salesInvoiceBean.getCurrentTransUserDetail().firstName} #{salesInvoiceBean.getCurrentTransUserDetail().secondName}"/>
                    <h:outputLabel value="Record added by : "/>
                    <h:outputLabel value="#{salesInvoiceBean.getCurrentUserDetail().firstName} #{salesInvoiceBean.getCurrentUserDetail().secondName}"/>
                </h:panelGrid>

                <h:panelGrid id="IpgrdSalesInvoiceFooter2" style="width: 350px; margin: 0px;border-bottom-style: dotted" columns="1" rendered="#{generalSetting.isTwoStringsEqual(companySetting.getIsShowDeveloper(),'Yes')}">
                    <h:outputLabel style="font-size: 10px;" value="System developed by WINGERsoft Technologies Ltd"/>
                    <h:outputLabel style="font-size: 10px;" value="#{companySetting.getDeveloperEmail()} | #{companySetting.getDeveloperPhone()}"/>
                </h:panelGrid>
            </h:panelGrid>
        </h:panelGrid>
        <!-- INVOICE ENDS HERE -->
    </h:body>
</html>

