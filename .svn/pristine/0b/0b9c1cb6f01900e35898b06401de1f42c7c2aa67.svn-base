<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>Transaction View</title>
        <link rel="stylesheet" type="text/css" href="style.css"/> 
    </h:head>

    <h:body>
        <p:commandButton id="cmdbPrint" styleClass="clsButtonBigLonger" value="Print Sales Invoice Below (ALT+P)" icon="ui-icon-print" type="button" accesskey="p">
            <p:printer target="TransactionGrid" rendered="false"/>
        </p:commandButton>
        <h:panelGrid id="TransactionGrid" style="border: solid black;"> 
            <h:panelGrid columns="3" style="margin: 0px;border-bottom-style: dotted;">
                <h:panelGrid style="width: 350px; margin: 0px;" columns="2">
                    <p:graphicImage value="#{companySetting.getLogoUrl()}" style="width:150px;float: left;" rendered="#{generalSetting.isTwoStringsEqual(companySetting.getShowLogoInvoice(),'Yes')}" />
                    <h:outputLabel value="#{companySetting.getPhysicalAddress()}"/>
                </h:panelGrid>

                <h:panelGrid style="width: 350px;text-align: center;" columns="1">
                    <h:outputLabel style="font-size: 20px;font-weight: bold;" value="#{companySetting.getLICENSE_CLIENT_NAME()}"/>
                    <h:outputLabel style="font-size: 16px;" value="#{companySetting.getBranchCode()} (#{generalUserSetting.currentStore.storeName})"/>
                </h:panelGrid>

                <h:panelGrid style="margin: 0px;" columns="1">
                    <h:outputLabel value=" Tel:#{companySetting.getPhone()} "/>
                    <h:outputLabel value=" TIN:#{companySetting.getTaxIdentity()} "/>
                    <h:outputLabel value=" Email:#{companySetting.getEmail()} "/>
                </h:panelGrid>
            </h:panelGrid>

            <h:panelGrid styleClass="clsGridMainInput" columns="6">

                <h:outputLabel styleClass="clsLabel" value="#{companySetting.getSalesReceiptName()}:" style="font-weight: bold;"/>
                <h:inputText styleClass="clsInput" id="itxtvvTransactionId" disabled="true" value="#{salesInvoiceBean.getCurrentTransaction().transactionId}" style="font-weight: bold;font-size: 14px;"/>

                <h:outputLabel styleClass="clsLabel" value="From.Store:"/>
                <h:inputText styleClass="clsInput" id="itxtvFromStore" disabled="true" value="#{storeBean.getStore(salesInvoiceBean.getCurrentTransaction().storeId).storeName}"/>

                <h:outputLabel styleClass="clsLabel" value="To.Store:"/>
                <h:inputText styleClass="clsInput" id="itxtvToStore" disabled="true" value="#{storeBean.getStore(salesInvoiceBean.getCurrentTransaction().store2Id).storeName}"/>

                <h:outputLabel styleClass="clsLabel" value="Add.Date:"/>
                <h:inputText styleClass="clsInput" id="itxtvTransactionDate" disabled="true" value="#{salesInvoiceBean.getCurrentTransaction().addDate}">
                    <f:convertDateTime pattern="#{companySetting.getDateFormat()} H:mm" timeZone="#{companySetting.getTimeZone()}"/>
                </h:inputText>

                <h:outputLabel value="Added.by: "/>
                <h:inputText styleClass="clsInput" id="itxtvAddUser" disabled="true" value="#{salesInvoiceBean.getCurrentUserDetail().firstName} #{salesInvoiceBean.getCurrentUserDetail().secondName}"/>

                <h:outputLabel styleClass="clsLabel" value="Offered.By:"/>
                <h:inputText styleClass="clsInput" id="itxtTransUser" disabled="true" value="#{userDetailBean.getUserDetail(salesInvoiceBean.getCurrentTransaction().transactionUserDetailId).firstName} #{userDetailBean.getUserDetail(trans.transactionUserDetailId).secondName}"/>

                <h:outputLabel styleClass="clsLabel" value="Trans.Type:"/>
                <h:inputText styleClass="clsInput" id="itxtvTransactionType" disabled="true" value="#{transactionTypeBean.getTransactionType(salesInvoiceBean.getCurrentTransaction().transactionTypeId).transactionTypeName}"/>

                <h:outputLabel styleClass="clsLabel" value="Trans.Reason:"/>
                <h:inputText styleClass="clsInput" id="itxtvTransactionReason" disabled="true" value="#{transactionReasonBean.getTransactionReason(salesInvoiceBean.getCurrentTransaction().transactionReasonId).transactionReasonName}"/>

                <h:outputLabel styleClass="clsLabel" value="Trans.Ref:"/>
                <h:inputText styleClass="clsInput" id="itxtvTransactionRef" disabled="true" value="#{salesInvoiceBean.getCurrentTransaction().transactionRef}"/>


                <h:outputLabel styleClass="clsLabel" value="Customer:"/>
                <h:inputText styleClass="clsInput" id="itxtvCustomerName" disabled="true" value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().transactorId).transactorNames}"/>

                <h:outputLabel styleClass="clsLabel" value="Bill.For:"/>
                <h:inputText styleClass="clsInput" id="itxtCustomerName2" disabled="true" value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().billTransactorId).transactorNames}"/>


                <h:outputLabel styleClass="clsLabel" value="Scheme.Name:"/>
                <h:inputText styleClass="clsInput" disabled="true" value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().schemeTransactorId).transactorNames}"/>
                <h:outputLabel styleClass="clsLabel" value="Princ.Scheme.Member:"/>
                <h:inputText styleClass="clsInput" disabled="true" value="#{salesInvoiceBean.getCurrentTransaction().princSchemeMember}"/>
            </h:panelGrid>

            <p:dataTable rowIndexVar="rowIndex" id="dtblvTransactionItem" value="#{transItemBean.getTransItemsByTransactionId2(generalUserSetting.getCurrentTransactionId())}" var="ti2" 
                         styleClass="cls-table" tableStyle="width:auto;" 
                         >  
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">#</f:facet>
                    <!-- row record -->
                    #{rowIndex+1}
                </p:column>
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Category</f:facet>
                    <!-- row record -->
                    #{categoryBean.getCategory(itemBean.getItem(ti2.itemId).categoryId).categoryName}
                </p:column>
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Item Description</f:facet>
                    <!-- row record -->
                    #{itemBean.getItem(ti2.itemId).description} [#{salesInvoiceBean.getVatRatedCode(ti2.vatRated)}]
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Unit</f:facet>
                    <!-- row record -->
                    #{unitBean.getUnit(itemBean.getItem(ti2.itemId).unitId).unitSymbol}
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Qty</f:facet>
                    <!-- row record -->
                    <h:outputLabel value="#{ti2.itemQty}" style="font-size: 14px;">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Unit Price</f:facet>
                    <!-- row record -->
                    <h:outputLabel value="#{ti2.unitPriceExcVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Unit VAT</f:facet>
                    <!-- row record -->
                    <h:outputLabel value="#{ti2.unitVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Unit Trade.Disc</f:facet>
                    <!-- row record -->
                    <h:outputLabel value="#{ti2.unitTradeDiscount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Amount</f:facet>
                    <!-- row record -->
                    <h:outputLabel value="#{ti2.amountIncVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </p:column>

            </p:dataTable>

            <h:panelGrid id="pgrdvTotals" styleClass="clsGridMainInput" columns="6">
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Sub.Total"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Total.Trade.Disc"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Total.VAT"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Cash.Disc"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="GRAND.TOTAL"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value=""/>

                <h:inputText id="itxtvSubTotal" value="#{salesInvoiceBean.getCurrentTransaction().subTotal}" styleClass="clsInputTransItem" style="width: 150px" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvTotalTradeDiscount" value="#{salesInvoiceBean.getCurrentTransaction().totalTradeDiscount}" styleClass="clsInputTransItem" style="width: 150px" autocomplete="off" disabled="true"> 
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvTotalVat" value="#{salesInvoiceBean.getCurrentTransaction().totalVat}" styleClass="clsInputTransItem" style="width: 150px" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvCashDiscount" value="#{salesInvoiceBean.getCurrentTransaction().cashDiscount}" styleClass="clsInputTransItem" style="width: 150px" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvGrandTotal" value="#{salesInvoiceBean.getCurrentTransaction().grandTotal}" styleClass="clsInputTransItem" style="width: 150px" disabled="true">
                    <f:convertNumber pattern="###,###,###"/>
                </h:inputText>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="#{companySetting.getCurrencyUsed()}">
                </h:outputLabel>
            </h:panelGrid>

            <h:panelGrid id="pgrdvPayment" styleClass="clsGridMainInput" columns="5">
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Payment.Method"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Amount.Tendered"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Spent.PointsAmt"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value="Change.Amount"/>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value=""/>

                <h:inputText styleClass="clsInput" id="itxtvPayMethod" disabled="true" value="#{payMethodBean.getPayMethod(salesInvoiceBean.getCurrentPay().payMethodId).payMethodName}"/>
                <h:inputText id="itxtvAmountTendered" value="#{salesInvoiceBean.getCurrentTransaction().amountTendered}" styleClass="clsInputTransItem" style="width: 150px;background-color: gold;" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvSpendPointsAmount" value="#{salesInvoiceBean.getCurrentPay().pointsSpentAmount}" styleClass="clsInputTransItem" style="width: 150px;background-color: aquamarine" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:inputText id="itxtvChange" value="#{salesInvoiceBean.getCurrentTransaction().changeAmount}" styleClass="clsInputTransItem" style="width: 150px;background-color: red;color: white" disabled="true">
                    <f:convertNumber pattern="###,###.##"/>
                </h:inputText>
                <h:outputLabel styleClass="clsLabelTransItem" style="width: 150px" value=""/>
            </h:panelGrid>
            <h:panelGrid columns="2">
                <h:panelGrid id="pgrdSalesInvoiceVAT" style="width: 350px; margin: 0px;" columns="4">
                    <h:outputLabel style="font-size: 14px;" value="VAT Rated"/>
                    <h:outputLabel style="font-size: 14px;" value="(%)"/>
                    <h:outputLabel style="font-size: 14px;" value="VatableAmount"/>
                    <h:outputLabel style="font-size: 14px;" value="VAT"/>

                    <h:outputLabel style="font-size: 14px;" value="Standard[S]"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().vatPerc}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalStdVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalVat}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>

                    <h:outputLabel style="font-size: 14px;" value="Zero[Z]"/>
                    <h:outputLabel style="font-size: 14px;" value="0"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalZeroVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="0"/>

                    <h:outputLabel style="font-size: 14px;" value="Exempt[E]"/>
                    <h:outputLabel style="font-size: 14px;" value="n/a"/>
                    <h:outputLabel style="font-size: 14px;" value="#{salesInvoiceBean.getCurrentTransaction().totalExemptVatableAmount}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="0"/>

                </h:panelGrid>

                <h:panelGrid id="pgrdSalesInvoicePointsCard" style="width: 350px; margin: 0px;" columns="2">
                    <h:outputLabel style="font-size: 14px;" value="Card No :"/>
                    <h:outputLabel style="font-size: 14px;" id="olblSalesInvoiceCardNo" value="#{salesInvoiceBean.getCurrentPointscard().cardNumber}"/>
                    <h:outputLabel style="font-size: 14px;" value="Card Holder :"/>
                    <h:outputLabel style="font-size: 14px;" id="olblSalesInvoiceCardHolder" value="#{salesInvoiceBean.getCurrentPointscard().cardHolder}"/>
                    <h:outputLabel style="font-size: 14px;" value="Points Spent :"/>
                    <h:outputLabel style="font-size: 14px;" id="olblSalesInvoicePointsSpent" value="#{salesInvoiceBean.getCurrentPay().pointsSpent}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="Points Awarded :"/>
                    <h:outputLabel style="font-size: 14px;" id="olblSalesInvoicePointsAwarded" value="#{salesInvoiceBean.getCurrentTransaction().pointsAwarded}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                    <h:outputLabel style="font-size: 14px;" value="Points Balance :"/>
                    <h:outputLabel style="font-size: 14px;" id="olblSalesInvoicePointsBalance" value="#{salesInvoiceBean.getCurrentPointscard().pointsBalance}">
                        <f:convertNumber pattern="###,###.##"/>
                    </h:outputLabel>
                </h:panelGrid>
            </h:panelGrid>
        </h:panelGrid> 
    </h:body>
</html>

