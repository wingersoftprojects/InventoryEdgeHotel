<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>Transaction Type</title>
        <link rel="stylesheet" type="text/css" href="style.css"/>
        <script  language="javascript">
            function confirmDelete()
            {
                return confirm('Are you sure you want to delete this current record?');
            }
            function alertSave()
            {
                return alert('Record saved successfully!');
            }
        </script> 
    </h:head>
    <f:event listener="#{navigationBean.checkAccessDenied('SETTING', 'View')}" type="preRenderView" />
    <h:body>
        <ui:include src="Menu.xhtml"/>
        <h:form id="formx" styleClass="clsMainFrom">

            <h:panelGrid styleClass="clsGridMainHeader" columns="1">
                <h:outputLabel styleClass="clsLabelTop" id="olblTransactionTypeHeader" value="Transaction Type"/>
            </h:panelGrid>

            <p:dataTable id="dtblTransactionType" value="#{transactionTypeBean.transactionTypes}" var="t"> 
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Transaction Type Name</f:facet>
                    <!-- row record -->
                    #{t.transactionTypeName}
                </p:column>
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Label for the Transactor</f:facet>
                    <!-- row record -->
                    #{t.transactorLabel}
                </p:column>
                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Label for the Transaction Number</f:facet>
                    <!-- row record -->
                    #{t.transactionNumberLabel}
                </p:column>

                <p:column>
                    <!-- column header -->
                    <f:facet name="header">Label for the Transaction Output</f:facet>
                    <!-- row record -->
                    #{t.transactionOutputLabel}
                </p:column>
            </p:dataTable>
        </h:form>
    </h:body>
</html>

