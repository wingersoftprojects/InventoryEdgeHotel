<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:p="http://primefaces.org/ui"
      xmlns:f="http://xmlns.jcp.org/jsf/core">
    <h:head>
        <title>Purchase Price History</title>
        <link rel="stylesheet" type="text/css" href="style.css"/>
    </h:head>
    <h:body>
        <h:form id="formReport">
            
            <p:dataTable tableStyle="width:auto;" id="tblTransactionItem" var="rts" value="#{transItemBean.getReportTransItemPriceHistory()}" 
                         >
                    <p:column headerText="UnitPrice">
                        <h:outputText value="#{rts.unitPriceExcVat}">
                            <f:convertNumber pattern="###,###.##"/>
                        </h:outputText>
                    </p:column>
                
                    <p:column headerText="Supplier">
                        <h:outputText value="#{rts.transactorNames}">
                        </h:outputText>
                    </p:column>
                    
                    <p:column headerText="Date">
                        <h:outputText value="#{rts.addDate}">
                            <f:convertDateTime pattern="#{companySetting.getDateFormat()}" timeZone="#{companySetting.getTimeZone()}"/>
                        </h:outputText>
                    </p:column>
                    
                    <p:column headerText="Qty">
                        <h:outputText value="#{rts.itemQty}">
                            <f:convertNumber pattern="###,###"/>
                        </h:outputText>
                    </p:column>
                
                    <p:column headerText="UnitVat">
                        <h:outputText value="#{rts.unitVat}">
                            <f:convertNumber pattern="###,###.##"/>
                        </h:outputText>
                    </p:column>
                    
                    <p:column headerText="UnitTradeDisc">
                        <h:outputText value="#{rts.unitTradeDiscount}">
                            <f:convertNumber pattern="###,###.##"/>
                        </h:outputText>
                    </p:column>
                    
                    <p:column headerText="Amount">
                        <h:outputText value="#{rts.amountIncVat}">
                            <f:convertNumber pattern="###,###.##"/>
                        </h:outputText>
                    </p:column>
                
                    <p:column headerText="Ref">
                        <h:outputText value="#{transBean.getTrans(rts.transactionId).transactionRef}">
                        </h:outputText>
                    </p:column>
                    
             </p:dataTable>
        </h:form>
    </h:body>
</html>

