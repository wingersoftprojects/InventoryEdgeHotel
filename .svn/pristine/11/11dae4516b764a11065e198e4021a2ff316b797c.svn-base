<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>Goods Delivery Note</title>
        <link rel="stylesheet" type="text/css" href="style.css"/> 
    </h:head>

    <h:body>
        <h:form id="TransactionForm">  
            <h:panelGrid id="TransactionGrid" style="border-style: solid;"> 
                <h:panelGrid columns="3" style="width:100%;margin: 0px;">
                    <h:panelGrid style="width: 350px; margin: 0px;" columns="2">
                        <p:graphicImage value="#{companySetting.getLogoUrl()}" style="width:150px;float: left;" rendered="#{generalSetting.isTwoStringsEqual(companySetting.getShowLogoInvoice(),'Yes')}" />
                        <h:outputLabel value="#{companySetting.getPhysicalAddress()}"/>
                    </h:panelGrid>
                    <h:panelGrid style="width: 350px;text-align: center;" columns="1">
                        <h:outputLabel style="font-size: 20px;font-weight: bold;" value="#{companySetting.getLICENSE_CLIENT_NAME()}"/>
                        <h:outputLabel style="font-size: 16px;" value="#{companySetting.getBranchCode()} (#{storeBean.getStore(salesInvoiceBean.getCurrentTransaction().storeId).storeName})"/>
                    </h:panelGrid>
                    <h:panelGrid style="margin: 0px;" columns="1">
                        <h:outputLabel value=" Tel:#{companySetting.getPhone()} "/>
                        <h:outputLabel value=" TIN:#{companySetting.getTaxIdentity()} "/>
                        <h:outputLabel value=" Email:#{companySetting.getEmail()} "/>
                    </h:panelGrid>
                </h:panelGrid>

                <h:panelGrid columns="1" style="text-align: center; width:100%;margin: 0px;background-color: gray;color: white;-webkit-print-color-adjust: exact;">
                    <h:outputLabel value="#{transactionTypeBean.getTransactionType(12).transactionOutputLabel}" style="font-size:30px;font-weight: bold;"/>
                    <h:outputLabel value="#{transactionTypeBean.getTransactionType(12).transactionNumberLabel}:#{salesInvoiceBean.getCurrentTransaction().transactionNumber}" style="font-weight: bold;"/>
                </h:panelGrid>

                <h:panelGrid columns="3" style="width:100%;margin: 0px;">
                    <h:panelGrid style="margin: 0px;text-align:left;" columns="1">
                        <h:outputLabel value="#{transactionTypeBean.getTransactionType(12).transactorLabel}" style="border-bottom-style: solid;"/>
                        <h:outputLabel value="ID:#{salesInvoiceBean.getCurrentTransaction().transactorId}"/>
                        <h:outputLabel value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().transactorId).transactorNames}"/>
                        <h:outputLabel value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().transactorId).physicalAddress}"/>
                        <h:outputLabel value="#{transactorBean.getTransactor(salesInvoiceBean.getCurrentTransaction().transactorId).phone}"/>
                    </h:panelGrid>
                    <h:panelGrid style="margin: 0px;text-align: left;" columns="1">
                        <h:outputLabel value="Sales.Invoice.Ref" style="border-bottom-style: solid;"/>
                        <h:outputLabel value="#{salesInvoiceBean.getCurrentTransaction().transactionRef}"/>
                    </h:panelGrid>
                    <h:panelGrid style="margin: 0px;text-align: left;" columns="1">
                        <h:outputLabel value="Delivery from" style="border-bottom-style: solid;"/>
                        <h:outputLabel value="#{storeBean.getStore(salesInvoiceBean.getCurrentTransaction().storeId).storeName}"/>
                    </h:panelGrid>
                </h:panelGrid>

                <p:dataTable rowIndexVar="rowIndex" id="dtblvTransactionItem" value="#{transItemBean.getTransItemsByTransactionId(generalUserSetting.getCurrentTransactionId())}" var="ti2" 
                             styleClass="cls-table" tableStyle="width:auto;" 
                             >  
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">#</f:facet>
                        <!-- row record -->
                        #{rowIndex+1}
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Item Description</f:facet>
                        <!-- row record -->
                        #{itemBean.getItem(ti2.itemId).description}
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Unit</f:facet>
                        <!-- row record -->
                        #{unitBean.getUnit(itemBean.getItem(ti2.itemId).unitId).unitSymbol}
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Batch No</f:facet>
                        <!-- row record -->
                        #{ti2.batchno}
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Qty</f:facet>
                        <!-- row record -->
                        <h:outputLabel value="#{ti2.itemQty}" style="font-size: 14px;">
                            <f:convertNumber pattern="###,###.##"/>
                        </h:outputLabel>
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Manuf.Date</f:facet>
                        <!-- row record -->
                        <h:outputLabel value="#{ti2.itemMnfDate}">
                            <f:convertDateTime pattern="#{companySetting.getDateFormat()}" timeZone="#{companySetting.getTimeZone()}"/>
                        </h:outputLabel>
                    </p:column>
                    <p:column>
                        <!-- column header -->
                        <f:facet name="header">Expiry.Date</f:facet>
                        <!-- row record -->
                        <h:outputLabel value="#{ti2.itemExpryDate}">
                            <f:convertDateTime pattern="#{companySetting.getDateFormat()}" timeZone="#{companySetting.getTimeZone()}"/>
                        </h:outputLabel>
                    </p:column>
                </p:dataTable>

                <h:panelGrid styleClass="clsGridMainInput" columns="2" style="width: 100%">
                    <h:outputLabel styleClass="clsLabelTransItem" style="border-bottom-style: solid;" value="Delivery Date"/>
                    <h:outputLabel styleClass="clsInput" style="border-bottom-style: solid;" value="Delivery Address"/>
                    <h:outputText styleClass="clsInput" value="#{salesInvoiceBean.getCurrentTransaction().deliveryDate}">
                        <f:convertDateTime pattern="#{companySetting.getDateFormat()}" timeZone="#{companySetting.getTimeZone()}"/>
                    </h:outputText>
                    <div style="width: 250px;">
                        #{salesInvoiceBean.getCurrentTransaction().deliveryAddress}
                    </div>
                </h:panelGrid>
            </h:panelGrid>
            <h:panelGrid>
                <p:commandButton id="cmdbvPrint" value="PRINT TRANSACTION" icon="ui-icon-print" type="button">
                    <p:printer target="TransactionGrid" rendered="false"/>
                </p:commandButton>
            </h:panelGrid>
        </h:form>
    </h:body>
</html>

